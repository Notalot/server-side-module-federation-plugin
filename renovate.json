{
  "branchPrefix": "renovate/",
  "dryRun": false,
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "logLevel": "info",
  "onboarding": true,
  "platform": "github",
  "includeForks": true,
  "repositories": ["kenotron/server-side-module-federation-plugin"],
  "postUpgradeTasks": {
    "commands": ["git add .", "npx beachball change --branch origin/main --type \"patch\" --message \"Renovate bump dependency version\""]
  },
  "trustLevel": "high",
  "allowedPostUpgradeCommands": ["^git add .", "^npx beachball"],
  "packageRules": [
    {
      "description": "lockFileMaintenance",
      "updateTypes": ["pin", "digest", "patch", "minor", "major", "lockFileMaintenance"],
      "masterIssueApproval": false,
      "stabilityDays": 0
    }
  ],
  "extends": [":npm"]
}
